
        <h4 class="mt-2">
         Retailers added by you </h4>
         <p><a href="/companies/public"> See the public list </a></p>
        

         <hr>

    <form action="/companies/create" method="post">
        <button class="mt-2 mb-2" type="submit"><img src="/images/add.png" width="20" height="20" alt="">Add a new retailer</button>
     </form>
      
      <% for(var i=com.length-1;i>=0;i--) { %>
         <div class="mb-3 p-3 rounded bg-primary" onclick="gotoCompany('<%= com[i].id %>')">
         <p>Name: <%= com[i].name %></p>
         <p>Company: <%= com[i].company %></p>
         <p>Status: <%= com[i].status %></p>
         <form action="/companies/remove/<%= com[i].id %>" method="post">
           <button class="mt-2" type="submit"><img src="/images/delete.png" width="20" height="20" alt="">Delete this retailer!</button>
        </form>
         </div>
      <% } %>
  
  